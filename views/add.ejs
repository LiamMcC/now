<% include ../partials/header %>
Edit Page
      
         <div class="w3-col l8 s12">
    
      <!-- Blog entry -->
      <div class="w3-container w3-white w3-margin w3-padding-large">

<form accept-charset="UTF-8" action="/add" method="post">
        <div class="w3-justify">
          <label for="name">Name</label>
          <p><input class="w3-input w3-border" type="text" name="name" Placeholder="Please enter a name"></p>
          <label for="image""">Image</label>
          <p><input class="w3-input w3-border" type="text" name="image" Placeholder="Please enter an image url"></p>
          <label for="price">Price</label>
          <p><input class="w3-input w3-border" type="text" name="price" Placeholder="Please enter a price"></p>
          <label for="activity">Activity</label>
          <p><input class="w3-input w3-border" type="text" name="activity" Placeholder="Please enter an activity"></p>
        
         <button class="w3-button w3-white w3-border">
           Post it
          </button >
          <p class="w3-clear"></p>

        </div>
        </form>
      </div>
      <hr>
      
    </div>
  
   

<!-- Subscribe Modal -->
<div id="subscribe" class="w3-modal w3-animate-opacity">
  <div class="w3-modal-content" style="padding:32px">
    <div class="w3-container w3-white">
      <i onclick="document.getElementById('subscribe').style.display='none'" class="fa fa-remove w3-transparent w3-button w3-xlarge w3-right"></i>
      <h2 class="w3-wide">Edit Item</h2>
      <p>Join my mailing list to receive updates on the latest blog posts and other things.</p>
      <p><input class="w3-input w3-border" type="text" placeholder="Enter e-mail"></p>
      <button type="button" class="w3-button w3-block w3-padding-large w3-red w3-margin-bottom" onclick="document.getElementById('subscribe').style.display='none'">Subscribe</button>
    </div>
  </div>
</div>


<% include ../partials/footer %>



